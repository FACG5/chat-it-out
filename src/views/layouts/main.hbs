<!DOCTYPE html>

<html lang="en">

<head>

    {{>head}}

</head>

<body>


    {{#if headerFound}}
    <header>
        {{>header}}
    </header>
    {{/if}}

    <main>

        {{{body}}}

    </main>
    {{#if asideFound}}
    <aside class="hide">

        {{>aside}}

    </aside>
    {{/if}}

    {{#if footerFound}}
    <footer>
        {{>footer}}
    </footer>
    {{/if}}
    {{#each javascript}}
    <script src="./js/{{this}}.js"></script>
    {{/each}}

</body>


</html>